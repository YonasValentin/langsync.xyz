{
  "name": "@langsync/expo",
  "version": "1.1.0",
  "description": "Expo/React Native integration for LangSync translation management with built-in UI components",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "react-native": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs"
    },
    "./storage": {
      "import": "./dist/storage/index.js",
      "require": "./dist/storage/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "pkgroll --minify",
    "dev": "pkgroll --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build && pnpm run test"
  },
  "keywords": [
    "translation",
    "i18n",
    "expo",
    "react-native",
    "langsync",
    "typescript"
  ],
  "author": "LangSync",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YonasValentin/langsync-packages.git",
    "directory": "expo"
  },
  "dependencies": {
    "@langsync/core": "workspace:*",
    "@langsync/client": "workspace:*"
  },
  "peerDependencies": {
    "expo": ">=50.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.72.0",
    "@react-native-async-storage/async-storage": ">=1.17.0",
    "@react-native-community/netinfo": ">=9.0.0"
  },
  "peerDependenciesMeta": {
    "@react-native-community/netinfo": {
      "optional": true
    }
  },
  "bugs": {
    "url": "https://github.com/YonasValentin/langsync-packages/issues"
  },
  "homepage": "https://langsync.xyz"
}
